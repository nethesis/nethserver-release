mirrorlist=http://mirrorlist.nethserver.org/?release=$releasever&repo=ce-extras&arch=$basearch&nsrelease=$nsrelease
#baseurl=http://mirror.centos.org/centos/$nsrelease/extras/$basearch/
gpgcheck=1
repo_gpgcheck=1
